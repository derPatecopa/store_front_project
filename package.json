{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "migrate-up": "db-migrate up --config database.json -e dev",
    "migrate-reset": "db-migrate reset --config database.json -e dev",
    "lint": "eslint . --ext .ts",
    "jasmine": "jasmine",
    "prettier": "npx prettier --write src/",
    "build": "npx tsc",
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess \"node ./build/server.js\"",
    "test": "npx tsc && export ENV=test && db-migrate --env test up && jasmine-ts && db-migrate reset --env test",
    "reset-test": "db-migrate reset --env test",
    "reset-dev": "db-migrate reset",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.9.0",
    "supertest": "^6.3.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/db-migrate-pg": "^0.0.10",
    "@types/eslint": "^8.21.1",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.7",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/nodemon": "^1.19.2",
    "@types/pg": "^7.14.11",
    "@types/prettier": "^2.7.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-init": "^1.1.0",
    "eslint": "^8.34.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
